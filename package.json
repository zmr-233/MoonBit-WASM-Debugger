{
  "name": "mbtmap",
  "displayName": "MoonBit WASM Debugger (Unofficial)",
  "description": "Provides precise line-number error display for MoonBit's WASM errors; UNTRACEABLE UNWRAPS NO MORE! (Works with the official plugin)",
  "repository": {
    "type": "git",
    "url": "https://github.com/zmr-233/MoonBit-WASM-Debugger.git"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "zmr-devtools",
  "license": "MIT",
  "main": "./dist/extension.js",
  "icon": "images/extension-icon.png",
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:moon.mod.json",
    "onTaskType:shell"
  ],
  "contributes": {
    "commands": [
      {
        "command": "mbtmap.clearErrors",
        "title": "Clean WASM Errors"
      }
    ]
  },
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "source-map": "^0.7.4"
  }
}
